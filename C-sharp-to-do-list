using System.Collections.Generics;

namespace to-do list
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("I've made a to-do list showing a list of items I need, here it is currently:");
            List<String> items = new List<String>();
            items.Add(fruits);
            items.Add(veggies);
            items.Add(shampoo);
            items.Add(Books);
            items.Add(pencils);
            items.Add(erasers);
            Console.WriteLine(items);

            thing = "";

            while(thing = "")
            {
                Console.WriteLine("Type in something that will be added to this to-do list.")
                thing = console.ReadLine();
            }

            items.Add(thing);
            Console.WriteLine("Here's the list with your new item:");
            Console.WriteLine(items);
        }
    }
}
